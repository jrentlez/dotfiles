{
	"$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
	"workspace": {
		"library": [
			"$VIMRUNTIME",
			"$HOME/.local/share/nvim/site/pack/core/opt/nvim-treesitter",
			"$HOME/.local/share/nvim/site/pack/core/opt/mini.diff",
			"$HOME/.local/share/nvim/site/pack/core/opt/mini-git",
			"$HOME/.local/share/nvim/site/pack/core/opt/mini.bracketed",
			"$HOME/.local/share/nvim/site/pack/core/opt/mini.pick"
		],
		"checkThirdParty": "false"
	},
	"runtime": {
		"path": [
			"lua/?.lua",
			"lua/?/init.lua"
		],
		"version": "LuaJIT"
	}
}
